{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "red",
          "powerline_symbol": "\ue0b0",
          "style": "plain",
          "template": "\uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "black",
          "background": "#FDE311",
          "template": " \uf07b {{ .Path }} ",
          "properties": {
            "style": "full",
            "folder_icon": "..\ue5fe..",
            "home_icon": "~"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "green",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{ if gt .Ahead 0 }}red{{ end }}",
            "{{ if gt .Behind 0 }}green{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_upstream_icon": true,
            "github_icon": "\uf408 ",
            "branch_icon": "\uE0A0 ",
            "fetch_status": true,
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": " on {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}<red> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<yellow> \uf046 {{ .Staging.String }}</>{{ end }}",
          "type": "git"
        },
        {
          "foreground": "#E4F34A",
          "leading_diamond": " | ",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "",
          "type": "python"
        },
        {
          "foreground": "#42E66C",
          "leading_diamond": " | ",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "",
          "type": "node"
        },
        {
          "foreground": "#ffffff",
          "leading_diamond": " | ",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": " \ue28e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "",
          "type": "flutter"
        },
        {
          "foreground": "#ffffff",
          "leading_diamond": " | ",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": " \uE798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "",
          "type": "dart"
        },
        {
          "type": "angular",
          "style": "diamond",
          "leading_diamond": " | ",
          "background": "#dd0730",
          "foreground": "white",
          "template": " \uE753 {{ .Full }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FDE311",
          "leading_diamond": "<#ff70a6> \ue200 </>",
          "properties": {
            "display_host": true
          },
          "style": "plain",
          "template": "{{ .HostName }} <#ffffff>on</> ",
          "type": "session"
        },
        {
          "foreground": "yellow",
          "properties": {
            "time_format": "02 Jan 06 15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background_templates": [
            "{{ if lt .Ms 60000 }}#C6FF00{{ end }}",
            "{{ if lt .Ms 3600000 }}#FFFF00{{ end }}",
            "{{ if lt .Ms 10800000 }}#FFD180{{ end }}",
            "{{ if gt .Ms 10800001 }}#FF867F{{ end }}"
          ],
          "foreground": "#212121",
          "leading_diamond": "\uE0B6",
          "properties": {
            "style": "austin",
            "threshold": 1
          },
          "style": "diamond",
          "template": "{{ if eq \"False\" (title (default \"False\" .Env.DISABLE_SEGMENT_PRIMARY_EXEC_TIME)) }}\uFA1E {{ .FormattedMs }}.{{ end }}",
          "trailing_diamond": "\uE0B4 ",
          "type": "executiontime"
        },
        {
          "foreground": "green",
          "style": "plain",
          "template": "ðŸš€",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
